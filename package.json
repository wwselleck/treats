{
  "name": "@wwselleck/treats",
  "version": "1.0.0",
  "main": "dist/server/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm -rf dist",
    "dev": "yarn clean && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn build:watch\" \"yarn dev:server\"",
    "dev:server": "nodemon \"dist/server/index.js\"",
    "lint": "prettier \"src/**/*\"",
    "mongo": "docker exec -it mongo mongo",
    "ping": "curl -X GET localhost:3000",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@types/mongoose": "^5.5.43",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "mongoose": "^5.8.10"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "typescript": "^3.7.4"
  }
}
