  "scripts": {
    "build": "tsc",
    "build:watch": "ls node_modules && tsc -w",
    "clean": "rm -rf dist",
    "dev": "yarn clean && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn build:watch\" \"yarn dev:server\"",
    "dev:server": "nodemon \"dist/server/index.js\"",
    "lint": "prettier \"src/**/*\"",
    "mongo": "docker exec -it mongo mongo treats-dev",
    "ping": "curl -X GET localhost:3218",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/inquirer": "^6.5.0",
    "@types/snoowrap": "^1.19.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cli-table": "^0.3.1",
    "commander": "^4.1.1",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "inquirer": "^7.0.6",
    "mongoose": "^5.8.10",
    "pino": "^5.16.0",
    "pino-pretty": "^3.6.0",
    "snoowrap": "^1.21.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-pino-logger": "^4.0.2",
    "@types/mongoose": "^5.5.43",
    "@types/pino": "^5.15.5",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "typescript": "^3.7.4"
  }
